{
    "rewrites": [
        {
            "source": "/gallery/pictures",
            "destination": "http://192.168.219.103:3000/gallery/pictures"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "./index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
        }
      ]
}